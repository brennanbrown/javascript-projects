{
  "name": "javascript-projects",
  "private": true,
  "version": "0.1.0",
  "description": "Static JS projects with a simple local dev server",
  "scripts": {
    "serve": "http-server -p 5173 -c-1 .",
    "serve:ssl": "http-server -S -C cert.pem -K key.pem -p 8443 -c-1 .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "prettier": "^3.3.3"
  }
}
